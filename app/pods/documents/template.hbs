{{#each model}}
	{{filename}} {{url}}<br>
{{/each}}
{{file-picker serviceType="text" store=store save="save" error="error"}}

{{#if errors.length}}
	<div class="errors">
	  <h2>Errors</h2>
	  <ul>
	    {{#each error in errors}}
	     <li>{{error}}</li>
	    {{/each}}
	  </ul>
	  <button class="btn" {{action 'clearErrors'}}>Clear Errors</button>
	</div>
{{/if}}